<?xml  version="1.0" encoding="ISO-8859-1"?>
<workflow>
	
	<property name="platformURI" value=".."/>
	
	<component class="org.openarchitectureware.xtext.parser.ParserComponent">
		<modelFile value="${modelFile}"/>
		<outputSlot value="${outputSlot}"/>
	</component>
	
	<component class="oaw.xtend.XtendComponent">
	   <metaModel id="mm" class="org.eclipse.m2t.type.emf.EmfRegistryMetaModel"/>
		<invoke value="org::openarchitectureware::xtext::XtextExtensions::fixTypes(${outputSlot})"/>
	</component>
	
	<component class="oaw.xtext.CheckComponent2">
	 	<metaModel idRef="mm"/>
       <expression value="${outputSlot}.eAllContents.union({${outputSlot}})"/>
	   <checkFile value="org::openarchitectureware::xtext::Checks"/>
	</component>
	
	<component class="oaw.xtend.XtendComponent">
	 	<metaModel idRef="mm"/>
		<invoke value="org::openarchitectureware::xtext::XtextExtensions::complete(${outputSlot})"/>
	</component>
	
</workflow>
