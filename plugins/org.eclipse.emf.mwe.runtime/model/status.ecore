<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="status"
    nsURI="http://www.eclipse.org/mwe/runtime/status" nsPrefix="status">
  <eClassifiers xsi:type="ecore:EClass" name="ExecutionStatus" eSuperTypes="#//AbstractExecutionStatus">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="message" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="statusType" eType="#//StatusType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="StatusType">
    <eLiterals name="SUCCESS" value="1"/>
    <eLiterals name="FAILURE" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="CompositeExecutionStatus" eSuperTypes="#//AbstractExecutionStatus">
    <eStructuralFeatures xsi:type="ecore:EReference" name="subStatus" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractExecutionStatus" abstract="true"/>
</ecore:EPackage>
